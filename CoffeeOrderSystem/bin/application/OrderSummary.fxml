<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPane" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.OrderSummaryController">
   <children>
      <Label fx:id="nameLabel" layoutX="14.0" layoutY="22.0" text="Label">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="orderLabel" layoutX="14.0" layoutY="92.0" text="Label">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="cancelOrderButton" layoutX="16.0" layoutY="161.0" mnemonicParsing="false" onAction="#cancelOrderClick" text="Cancel my order" />
      <Button fx:id="getCoffeeButton" layoutX="172.0" layoutY="161.0" mnemonicParsing="false" onAction="#getCoffeeClick" text="Get me my coffee!" />
	</children>
</AnchorPane>
